{
  "name": "reqwq",
  "version": "0.3.0",
  "description": "A reactivity state manager of React.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "README.md",
    "README.zh.md",
    "babel-plugin.js"
  ],
  "browserslist": [
    "ie 9",
    "safari 6"
  ],
  "scripts": {
    "start": "parcel index.html",
    "test": "ava",
    "cov": "nyc ava",
    "report": "nyc --reporter=lcov ava",
    "coverage": "codecov",
    "lint": "eslint *.{ts,js,tsx}",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && ava",
    "devBabel": "ts-node -O \"{\\\"module\\\":\\\"commonjs\\\"}\" devBabel.ts",
    "build": "del-cli dist && bili index.ts --format cjs --format umd --format esm --external react --module-name reqwq && tsc -m commonjs --removeComments"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && ava"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShirasawaSama/reqwq.git"
  },
  "keywords": [
    "react",
    "hooks",
    "proxy",
    "state",
    "global",
    "store"
  ],
  "author": "Shirasawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShirasawaSama/reqwq/issues"
  },
  "homepage": "https://github.com/ShirasawaSama/reqwq#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/traverse": "^7.9.5",
    "@babel/types": "^7.9.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/babel__core": "^7.1.7",
    "@types/babel__traverse": "^7.0.10",
    "@types/enzyme": "^3.10.5",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "ava": "^3.7.1",
    "bili": "^4.9.1",
    "codecov": "^3.6.5",
    "del-cli": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25",
    "husky": "^4.2.5",
    "jsdom": "^16.2.2",
    "nyc": "^15.0.1",
    "parcel": "^1.12.4",
    "react": "^16.13.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.13.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "source-map-support": "^0.5.18",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "ava": {
    "require": [
      "source-map-support/register",
      "ts-node/register",
      "esm"
    ],
    "timeout": "2m",
    "extensions": [
      "tsx"
    ]
  },
  "nyc": {
    "exclude": [
      "test.tsx",
      "babel.ts"
    ],
    "extends": "@istanbuljs/nyc-config-typescript"
  }
}
